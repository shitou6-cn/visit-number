<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>com.shitou6.controller.test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>start0</servlet-name>
        <servlet-class>com.shitou6.controller.Start0</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>start1</servlet-name>
        <servlet-class>com.shitou6.controller.Start1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>insert</servlet-name>
        <servlet-class>com.shitou6.controller.Insert</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>init</servlet-name>
        <servlet-class>com.shitou6.controller.Init</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>start0</servlet-name>
        <url-pattern>/start0</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>start1</servlet-name>
        <url-pattern>/start1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>insert</servlet-name>
        <url-pattern>/insert</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>init</servlet-name>
        <url-pattern>/init</url-pattern>
    </servlet-mapping>


</web-app>